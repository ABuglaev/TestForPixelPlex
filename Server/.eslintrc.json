{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "extends": "airbnb-base",
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "prefer-destructuring": 1,
        "no-underscore-dangle": 0,
        "prefer-const": 0,
        "no-use-before-define": 0,
        "curly": 0,
        "consistent-return": 0
    }
}